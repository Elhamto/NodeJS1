extends base.pug

block title 
    title HomePage 

block content 
    ul(id="chat-list-container")
        if(chats)
            each chat in chats
                a(href=`/chat/${chat.GroupId}`)
                    div.chat-list-element
                        li=chat.Group.name
                        li= JSON.stringify(chat)
    block script
        script(src="js/main.js") 
